_G.Bin = true
while _G.Bin do

game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName","Moderator")
wait()
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName","Administrator")
wait()
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName","Owner")
wait()
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName","Head Admin")
wait()
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayName","Super admin")

end
